# Database Configuration
DATABASE_URL=postgresql://user:password@localhost:5432/filemanager

# AI Services
OPENAI_API_KEY=sk-...
DIFY_API_KEY=app-...
DIFY_BASE_URL=https://api.dify.ai/v1

# Google Cloud Storage
GOOGLE_CLOUD_CREDENTIALS={"type":"service_account","project_id":"...","private_key":"...","client_email":"..."}

# Storage Paths
PUBLIC_OBJECT_SEARCH_PATHS=/app/public
PRIVATE_OBJECT_DIR=/app/.private

# Application Settings
NODE_ENV=production
PORT=5000

# Optional: Session Secret (generate a random string)
SESSION_SECRET=your-random-session-secret-here

# Optional: API Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100